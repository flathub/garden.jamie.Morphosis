{
  "id": "garden.jamie.Morphosis",
  "runtime": "org.gnome.Platform",
  "runtime-version": "48",
  "sdk": "org.gnome.Sdk",
  "command": "morphosis",
  "finish-args": [
    "--share=ipc",
    "--socket=fallback-x11",
    "--device=dri",
    "--socket=wayland"
  ],
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/man",
    "/share/doc",
    "/share/gtk-doc",
    "/share/man",
    "/share/pkgconfig",
    "*.la",
    "*.a"
  ],
  "modules": [
    "python3-weasyprint.json",
    "pandoc.json",
    {
      "name": "blueprint-compiler",
      "buildsystem": "meson",
      "cleanup": ["*"],
      "sources": [
        {
          "type": "git",
          "url": "https://gitlab.gnome.org/jwestman/blueprint-compiler",
          "tag": "v0.16.0"
        }
      ]
    },
    {
      "name": "morphosis",
      "builddir": true,
      "buildsystem": "meson",
      "sources": [
        {
          "type": "git",
          "url": "https://gitlab.gnome.org/World/morphosis.git",
          "tag": "48.1",
          "commit": "d401e5289f0e448a9ef6d1d2e3cdb03f015868f7"
        }
      ]
    }
  ]
}
